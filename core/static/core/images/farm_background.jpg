<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<svg xmlns="http://www.w3.org/2000/svg" width="1200" height="800" viewBox="0 0 1200 800">
  <!-- Sky background -->
  <rect width="1200" height="500" fill="#87CEEB"/>
  
  <!-- Sun -->
  <circle cx="1000" cy="150" r="80" fill="#FFD700"/>
  
  <!-- Mountains -->
  <polygon points="0,500 200,300 400,450 600,250 800,400 1000,200 1200,500" fill="#6B8E23"/>
  
  <!-- Fields -->
  <rect x="0" y="500" width="1200" height="300" fill="#228B22"/>
  
  <!-- Field patterns -->
  <g fill="#32CD32" opacity="0.7">
    <rect x="50" y="520" width="200" height="100"/>
    <rect x="300" y="550" width="250" height="150"/>
    <rect x="600" y="520" width="300" height="120"/>
    <rect x="950" y="540" width="200" height="160"/>
  </g>
  
  <!-- Farm house -->
  <g>
    <rect x="100" y="400" width="150" height="100" fill="#8B4513"/>
    <polygon points="100,400 175,320 250,400" fill="#A52A2A"/>
    <rect x="150" y="450" width="40" height="50" fill="#1E90FF"/>
  </g>
  
  <!-- Trees -->
  <g>
    <circle cx="400" cy="420" r="30" fill="#006400"/>
    <rect x="395" y="420" width="10" height="40" fill="#8B4513"/>
    
    <circle cx="800" cy="450" r="40" fill="#006400"/>
    <rect x="795" y="450" width="10" height="50" fill="#8B4513"/>
    
    <circle cx="900" cy="430" r="35" fill="#006400"/>
    <rect x="895" y="430" width="10" height="45" fill="#8B4513"/>
  </g>
  
  <!-- Crops -->
  <g fill="#DAA520">
    <rect x="500" y="600" width="5" height="20"/>
    <rect x="520" y="600" width="5" height="20"/>
    <rect x="540" y="600" width="5" height="20"/>
    <rect x="560" y="600" width="5" height="20"/>
    <rect x="580" y="600" width="5" height="20"/>
    <rect x="500" y="640" width="5" height="20"/>
    <rect x="520" y="640" width="5" height="20"/>
    <rect x="540" y="640" width="5" height="20"/>
    <rect x="560" y="640" width="5" height="20"/>
    <rect x="580" y="640" width="5" height="20"/>
  </g>
</svg>